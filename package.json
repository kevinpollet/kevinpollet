{
  "name": "kevinpollet",
  "description": "npm card for Kevin Pollet (@kevinpollet)",
  "version": "1.2.1",
  "author": "kevinpollet <pollet.kevin@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/kevinpollet/kevinpollet#readme",
  "bugs": "https://github.com/kevinpollet/kevinpollet/issues",
  "repository": "github:kevinpollet/kevinpollet",
  "keywords": [
    "card",
    "npm",
    "npm card",
    "npx",
    "npx card"
  ],
  "bin": "bin/card.js",
  "files": [
    "bin"
  ],
  "scripts": {
    "build": "rm -f bin/output && node scripts/build.js",
    "dev": "node bin/card.js",
    "format": "prettier --write '{scripts,bin,.github}/**/*.{js,yml}' '*.{json,md}'",
    "predev": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "boxen": "^4.1.0",
    "chalk": "^2.4.2",
    "husky": "^3.0.8",
    "prettier": "1.18.2",
    "pretty-quick": "^1.11.1"
  },
  "prettier": {
    "trailingComma": "es5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
